{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "http:default",
    "http:allow-fetch",
    "fs:default",
    "fs:allow-read",
    "fs:allow-write",
    "shell:default",
    "shell:allow-execute",
    "store:default"
  ]
}
